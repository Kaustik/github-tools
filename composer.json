{
  "name": "kaustik/github-tools",
  "type": "System",
  "description": "Scripts for github.",
  "license": "MIT",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/Kaustik/github-php-client.git"
    },
    {
      "type": "git",
      "url": "https://github.com/Kaustik/phuml.git"
    }
  ],
  "require": {
    "symfony/framework-bundle": "~2.3|~3.0",
    "tan-tan-kanarek/github-php-client" : "dev-master",
    "symfony/console": "3.*",
    "PhUML": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {
      "Kaustik\\": "src/Kaustik",
      "Test\\": "tests"
    }
  },
  "bin" : ["scripts/s3signature_and_upload.sh"]
}